{
    "manifest_version": 3,
    "name": "FTC Matches",
    "description": "Lists a team's matches in a FTC event \"on deck\" / \"next up\" counter.",
    "version": "0.5.0",
    "action":{
        "default_popup": "extension.html"
    },
    "host_permissions": [
        "https://ftc-api.firstinspires.org/v2.0/*"
    ] ,
    "background":{
        "service_worker": "scripts/service.js"
    }
}